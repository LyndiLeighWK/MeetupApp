<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Cannot_Exceed_Registration_Limit</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISNEW(), Meetup__r.Registrations__c &gt;=  Meetup__r.Registration_Limit__c)</errorConditionFormula>
    <errorMessage>Sorry, this Meetup is already full!</errorMessage>
</ValidationRule>
